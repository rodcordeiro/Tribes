{
  "expo": {
    "name": "Tribes",
    "slug": "Tribes",
    "scheme": "tribes",
    "platforms": [
      "ios",
      "android"
    ], 
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rodcordeiro.Tribes"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5736c04f-6827-4122-8cba-7b90161bfad4"
      }
    }
  }
}
